{
  "name": "cypress",
  "version": "1.0.0",
  "description": "Pruebas automatizadas apoyadas con cypress.",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open --e2e --browser chrome",
    "cypress:run": "cypress run --e2e --browser chrome --headless --spec \"cypress/e2e/ghost/*.cy.js\"",
    "test": "npm run cypress:run --",
    "format": "prettier --write cypress"
  },
  "keywords": [
    "cypress",
    "e2e"
  ],
  "author": "Juan Manuel González Garzón",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^13.8.0",
    "mochawesome": "^7.1.3",
    "prettier": "^3.2.5"
  }
}
